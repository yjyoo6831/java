package ex01;

public class Sub05 {
	public static void run() {
		// control loop
		int score=59;
		String grade = "";
		
		if (score>=90) {
			grade="A";
		}
		else if(score>=80){
			grade="B";
		}
		else if(score>=70) {
			grade="C";
		}
		else if(score>=60) {
			grade="D";
		}
		else {
			grade="넌 성적이 아냐";
		}
		System.out.printf("%d score : %s",score,grade);
	}
}
